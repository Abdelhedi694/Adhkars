{
  "expo": {
    "name": "Invocations Matin et Soir",
    "slug": "Invocations_Matin_et_Soir",
    "version": "1.0.5",
    "orientation": "portrait",
    "scheme": "myapp",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Invocations.MatinEtSoir"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1810776046585518~2880373886"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "INTERNET"
      ],
      "package": "com.abdelhedi.Invocations_Matin_et_Soir"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1810776046585518~2880373886",
          "iosAppId": "ca-app-pub-1810776046585518~6222419769"
        }
      ],
      "expo-tracking-transparency"
    ],
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-1810776046585518~2880373886",
      "ios_app_id": "ca-app-pub-1810776046585518~6222419769"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "91b47c98-8e74-4311-a750-32a820857b21"
      }
    }
  }
}